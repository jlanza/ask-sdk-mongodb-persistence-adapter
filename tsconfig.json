{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./lib",
    "outDir" : "dist",
    "sourceMap" : true,
    "alwaysStrict" : true,
    "declaration" : true,
    "lib": []
  },
  "include": [
    "lib"
  ],
    "typedocOptions": {
      "inputFiles": ["./lib/attributes/"],
        "mode": "modules",
        "out": "docs",
        "media": "media/"
    }
}
